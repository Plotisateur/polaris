{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:loreal-techaccelerator/project-template-front.git",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "20.x.x"
  },
  "type": "module",
  "scripts": {
    "install": "cd ./proxy && pnpm install",
    "dev": "vite",
    "dev:mock": "VITE_AXIOS_MOCK=true vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "node ./proxy/front-server.js",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "eslint './src/**/*' --ext .tsx,.ts --max-warnings 0 --ignore-pattern '**/*.css'",
    "lint:fix": "eslint './src/**/*' --ext .tsx,.ts --fix --max-warnings 0 --ignore-pattern '**/*.css'",
    "lint:css": "stylelint '**/*.css' --fix",
    "lint:staged": "eslint --max-warnings 0",
    "check-type": "tsc --noEmit",
    "check-all": "pnpm check-type && pnpm lint && pnpm test",
    "prettier": "prettier --config ../../.prettierrc '**/*.{json,js,jsx,ts,tsx,css,scss,md}' --write",
    "prettier:front": "prettier --config ./.prettierrc '**/*.{json,js,jsx,ts,tsx,css,scss,md}' --write"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-accordion": "^1.2.7",
    "@radix-ui/react-avatar": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.11",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-progress": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@react-oauth/google": "^0.12.1",
    "@tailwindcss/vite": "^4.1.4",
    "@tanstack/react-query": "^4.29.5",
    "@vitest/coverage-v8": "^3.1.2",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.8.4",
    "lodash": "^4.17.21",
    "lucide-react": "^0.503.0",
    "luxon": "^3.6.1",
    "numeral": "^2.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-gtm-module": "^2.0.11",
    "react-hook-form": "^7.56.0",
    "react-number-format": "^5.4.4",
    "react-router-dom": "^6.11.0",
    "react-toastify": "^9.1.3",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.8",
    "typescript": "^5.0.4",
    "universal-cookie": "^4.0.4",
    "yup": "^1.6.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@svgr/rollup": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.17.16",
    "@types/luxon": "^3.6.2",
    "@types/node": "^18.19.86",
    "@types/numeral": "^2.0.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@types/react-gtm-module": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "axios-mock-adapter": "^1.21.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "postcss": "^8.5.3",
    "prettier": "^2.8.8",
    "stylelint": "^16.19.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "^4.1.4",
    "tailwindcss-animate": "^1.0.7",
    "vite": "^5.0.10",
    "vitest": "^1.0.4"
  }
}
